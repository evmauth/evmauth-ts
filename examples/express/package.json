{
    "name": "@evmauth/express-example",
    "version": "0.1.0",
    "description": "Example usage of the EVMAuth SDK with Express.js",
    "private": true,
    "main": "dist/index.js",
    "type": "module",
    "engines": {
        "node": ">=18.0.0"
    },
    "license": "MIT",
    "scripts": {
        "build": "tsc",
        "check": "biome check --write . && biome format --write . && tsc --noEmit",
        "coverage": "vitest run --coverage",
        "lint": "biome check --write .",
        "lint:unsafe": "biome check --unsafe --write .",
        "format": "biome format --write .",
        "format:unsafe": "biome format --unsafe --write .",
        "start": "node dist/index.js",
        "test": "vitest --run",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "body-parser": "^2.2.0",
        "dotenv": "^16.5.0",
        "ethers": "^6.14",
        "evmauth": "^0.2.0",
        "express": "^5.1.0"
    },
    "devDependencies": {
        "@biomejs/biome": "^1.9.4",
        "@types/express": "^5.0.1",
        "typescript": "^5",
        "vitest": "^3.1.3"
    }
}
