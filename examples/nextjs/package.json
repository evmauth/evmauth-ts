        "@types/node": "20.17.1",
        "@types/react": "19.0.1",
        "@types/react-dom": "19.0.1",
        "@vitejs/plugin-react": "4.4.1",
        "autoprefixer": "^10.4.16",
        "jsdom": "26.1.0",
        "msw": "^2.0.11",
        "postcss": "^8.4.33",
        "tailwindcss": "^3.4.1",
