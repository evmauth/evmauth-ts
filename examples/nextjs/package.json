{
    "name": "@evmauth/nextjs-example",
    "version": "0.1.0",
    "description": "Example usage of the EVMAuth SDK with Next.js",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
